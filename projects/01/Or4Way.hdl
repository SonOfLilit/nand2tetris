/**
 * 4-way or gate: out = in[0] or in[1] or ... or in[3].
 */

CHIP Or4Way {
    IN in[4];
    OUT out;

    PARTS:
    Or(a=in[0], b=in[1], out=a);
    Or(a=in[2], b=in[3], out=b);
    Or(a=a, b=b, out=out);
}