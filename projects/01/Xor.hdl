/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=na);
    Not(in=b, out=nb);
    Nand(a=a, b=nb, out=oz);
    Nand(a=na, b=b, out=zo);
    Nand(a=oz, b=zo, out=out);
}